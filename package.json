{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cd frontend && vite && cd ..",
    "build": "cd frontend && vite build && cd ..",
    "preview": "cd frontend && vite preview && cd ..",
    "test:unit": "cd frontend && vitest && cd ..",
    "coverage": "cd frontend && vitest run --coverage && cd ..",
    "lint": "cd frontend && eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore && cd ..",
    "format": "cd frontend && prettier --write src/ && cd .."
  },
  "dependencies": {
    "axios": "^1.3.5",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.3.0",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.4",
    "vite": "^4.1.4",
    "vitest": "^0.30.0"
  }
}
